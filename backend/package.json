{
  "name": "fullstack-prisma-angular",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run db:generate && npm run tsoa && tsc --build",
    "start": "node build/index.js",
    "start:all": "npm run db:push && npm run db:seed && npm run start",
    "predev": "npm run tsoa",
    "dev": "set TS_NODE=TRUE&& nodemon --watch src src/index.ts",
    "tsoa": "tsoa spec-and-routes && npx openapi-typescript src/generated/swagger.json --output src/generated/schema.ts",
    "tsoa:mon": "nodemon --watch src --ignore src/generated -e ts,json -x npm run tsoa",
    "build:e2e": "npm run tsoa && tsc -p tsconfig.e2e.json",
    "start:e2e": "mocha --timeout 40000 --file build/e2e.js \"build/e2e/**/*.js\"",
    "e2e": "npm run build:e2e && npm run start:e2e",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:push": "npx prisma db push",
    "db:seed": "ts-node src/seed/seed.ts",
    "lint": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damiansimanuk/fullstack-prisma-angular.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/damiansimanuk/fullstack-prisma-angular/issues"
  },
  "homepage": "https://github.com/damiansimanuk/fullstack-prisma-angular#readme",
  "dependencies": {
    "@prisma/client": "^3.2",
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.2.1",
    "ts-simple-nameof": "^1.3.1",
    "tsoa": "^3.9.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.21",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mocha": "^7.0.2",
    "@types/node": "^16.4.13",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.12",
    "openapi-typescript": "^4.0.2",
    "prettier": "^2.3.2",
    "prisma": "^3.2",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.4",
    "typescript": "^4.3.5"
  }
}
